{
  "name": "eldutcho/solid-structure",
  "description": "A typical SOLID principles folder structure for projects.",
  "type": "project",
  "require": {
    "php": "^8.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.4.4"
  },
  "autoload": {
    "psr-4": {
      "App\\\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit"
  }
}
